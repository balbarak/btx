<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             Title="Welcome To Btx Chat"
             x:Class="Btx.Mobile.Views.LogoutPage">
    <ContentPage.Content>
        <StackLayout VerticalOptions="Start" Margin="10,50,10,10">

            <Button Text="Create new account" Command="{Binding GoToRegisterPageCommand}"/>
            
            <Button Text="Login" Command="{Binding GoToLoginPageCommand}"/>
            
        </StackLayout>
    </ContentPage.Content>
</ContentPage>